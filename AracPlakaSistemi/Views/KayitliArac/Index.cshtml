@model AracPlakaSistemi.ViewModels.Admin.KayitliAracSearchViewModel
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}
<title>Kayıtlı Araçlar</title>
<div class="row">
    <div class="col-xs-12">
        <div class="box box-solid">
            <div class="box-header">
                <i class="fa fa-book"></i>
                <h3 class="box-title">@ViewBag.Title</h3>
            </div>
            <div class="box-body">
                @using (Html.BeginForm("AracList", "KayitliArac", FormMethod.Post, new { role = "form", id = "kayitliAracSearchForm", data_ajax_form = "true", data_target_container = "#KayitliAracListContainer" }))
                {
                    <div class="row">
                        <div class="form-group form-group-sm col-xs-12 col-sm-6 col-md-4 col-lg-4 @Html.ModelStateFor(m => m.Plaka)">
                            @Html.TextBoxBlockLabelAsAddonFor(m => m.Plaka)
                        </div>
                      
                       
                        <div class="clearfix"></div>
                        <div class="form-group form-group-sm col-xs-12 col-sm-4 col-sm-push-3 col-md-4 col-md-push-3">
                            <div class="input-group">
                                <button type='submit' class="btn btn-sm btn-default btn-block"><i class="fa fa-search"></i></button>
                            </div>
                        </div>
                    </div>

                }
                <div>
                    <div id="KayitliAracListContainer"></div>
                </div>
            </div>
        </div>
    </div>
</div>
